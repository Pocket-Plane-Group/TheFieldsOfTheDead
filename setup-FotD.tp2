	  INT_VAR
	  header_type			= 1
	  speed					= (8 - "%enchantment%")
	  dicesize				= 6
	  dicenumber			= 2
	  END
	END
  PATCH_IF ("%appearance%" STRING_EQUAL_CASE "S3") THEN BEGIN // Katana
	WRITE_SHORT 0x8e "7"
	END
  PATCH_IF ("%appearance%" STRING_EQUAL_CASE "S1" AND "%proficiency%" = "95") THEN BEGIN // Ninjato
    WRITE_BYTE 0x84 "3"
	WRITE_SHORT 0x8e "7"
	END
