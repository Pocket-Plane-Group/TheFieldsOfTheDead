// Regular expression for stores //
  
COPY_EXISTING_REGEXP GLOB ~.*\.sto~ ~override~
  // Correct Buying Quantity & Total Amount For Ammunition & Throwing Weapons //
  READ_LONG 0x34 "itemsoffset"
  READ_LONG 0x38 "items"
  SET "currentitem" = 0
  WHILE ("%items%" > 0) BEGIN
    READ_ASCII ("%itemsoffset%" + ("%currentitem%" * 0x1c)) "filename"
	PATCH_IF ("%filename%" STRING_EQUAL_CASE "%tutu_var%arow01") BEGIN
	  WRITE_SHORT ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0xa) 20
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x18) 1
	END
	PATCH_IF ("%filename%" STRING_EQUAL_CASE "%tutu_var%arow04" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow05" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow08" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow09" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow10" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow63" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow69" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%bolt03" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%bolt04" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%bolt60" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%bull60" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%bull61" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%dart03" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%dart04") BEGIN
	  WRITE_SHORT ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0xa) 2
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x14) 10
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x18) 0
	END
	PATCH_IF ("%filename%" STRING_EQUAL_CASE "%tutu_var%arow06" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow07" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow64" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%bolt05") BEGIN
	  WRITE_SHORT ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0xa) 1
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x14) 5
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x18) 0
	END
	PATCH_IF ("%filename%" STRING_EQUAL_CASE "%tutu_var%arow11" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow60" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow61" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow62" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%bolt06" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%bull03" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%dart62" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%dart63" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%ax1h04") BEGIN
	  WRITE_SHORT ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0xa) 2
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x14) 20
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x18) 0
	END
	PATCH_IF ("%filename%" STRING_EQUAL_CASE "%tutu_var%arow65" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow66" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow67" OR "%filename%" STRING_EQUAL_CASE "%tutu_var%arow68") BEGIN
	  WRITE_SHORT ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0xa) 1
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x14) 5
	  WRITE_LONG ("%itemsoffset%" + ("%currentitem%" * 0x1c) + 0x18) 0
	END
