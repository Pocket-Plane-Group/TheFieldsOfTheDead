	WRITE_SHORT 0x8e "6"
  END
  PATCH_IF ("%category%" = "30" AND "%appearance%" STRING_EQUAL_CASE "HB") THEN BEGIN // Halberd
	WRITE_SHORT 0x8e "7"
  END
