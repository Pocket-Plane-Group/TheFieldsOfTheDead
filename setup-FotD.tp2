BEGIN ~Move Boo to the inventory~
GROUP ~Optional Tweaks~

COPY_EXISTING_REGEXP GLOB ~mins..*\.cre~ ~override~
  READ_LONG 0x2b8 "item_slots_offset"
  READ_LONG 0x2bc "items_offset"
  READ_LONG 0x2c0 "#items"
  SET "#read" = "0"
  FOR (i = %#items% ; i ; --i) BEGIN
    READ_ASCII ("%items_offset%" + ("0x14" * "%#read%")) "item"
	SET "#read" += "1"
	PATCH_IF ("%item%" STRING_EQUAL_CASE "misc84") THEN BEGIN
	  WRITE_SHORT ("%item_slots_offset%" + "36") "-1"
	  WRITE_SHORT ("%item_slots_offset%" + "42") ("%#read%" - "1")
	  END
	END
	BUT_ONLY
	
