IF
  HasItem("POTN62",Myself)  // ~Potion of Superior Healing~
  HPPercentLT(Myself,30)
  !GlobalTimerNotExpired("CastAndAttack","LOCALS")
THEN
  RESPONSE #100
    SetGlobalTimer("CastAndAttack","LOCALS",6)
    UseItem("POTN62",Myself)  // ~Potion of Superior Healing~
END

IF
  HasItem("POTN62",Myself)  // ~Potion of Superior Healing~
  HPPercentGT(Myself,30)
  Heard([EVILCUTOFF],5)
  !HasItem("POTN62",LastHeardBy(Myself))  // ~Potion of Superior Healing~
THEN
  RESPONSE #100
    GiveItem("POTN62",LastHeardBy(Myself))  // ~Potion of Superior Healing~
END

